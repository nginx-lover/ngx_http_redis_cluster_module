ngx_addon_name=ngx_http_redis_cluster_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=$ngx_addon_name
    ngx_module_srcs="$ngx_addon_dir/src/ngx_http_redis_cluster_module.c"

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/src/ngx_http_redis_cluster_module.c"
    CFLAGS="$CFLAGS -g -Wno-unused-variable"
    CORE_LIBS="$CORE_LIBS"
fi
